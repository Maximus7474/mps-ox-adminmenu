{
	"name": "mps-ox-adminmenu",
	"author": "Maximus7474",
	"version": "0.0.0",
	"license": "AGPL-3.0",
	"description": "A simple admin menu for ox_core to handle players, jobs, etc....",
	"type": "module",
  "workspaces": [
    "web"
  ],
	"scripts": {
    "web:dev": "bun --filter web dev",
    "build": "bun scripts/build.js",
    "watch": "bun run watch & bun run web:dev",
    "format": "bun biome format --write .",
    "lint": "bun biome lint --write ."
  },
	"keywords": [
		"fivem",
		"ox_core"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/Maximus7474/mps-ox-adminmenu.git"
	},
	"bugs": "https://github.com/Maximus7474/mps-ox-adminmenu/issues",
	"devDependencies": {
		"@citizenfx/client": "latest",
		"@citizenfx/server": "latest",
		"@jgoz/esbuild-plugin-typecheck": "^4.0.3",
		"@vitejs/plugin-react": "^4.3.4",
    "@types/node": "^22.13.13",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
		"esbuild": "^0.25.0",
		"typescript": "^5.8.2"
	},
	"dependencies": {
		"@biomejs/biome": "^1.9.4",
		"@communityox/fx-utils": "^0.0.4",
		"@communityox/ox_core": "^1.5.8",
		"@communityox/ox_lib": "^3.31.4",
		"@nativewrappers/fivem": "latest"
	}
}
